    "def factorial(n):\n",
    "  if(n==0):\n",
    "    return 1\n",
    "  \n",
    "  num=1\n",
    "\n",
    "  for i in range(1,n+1):\n",
    "    num*=i\n",
    "  return num\n",
    "\n",
    "N,K=map(int,input().split())\n",
    "\n",
    "mother=factorial(N)\n",
    "son=factorial(N-K)*factorial(K)\n",
    "result=mother//son\n",
    "result%=10007\n",
    "print(result)"
   ]
  }
 ],
