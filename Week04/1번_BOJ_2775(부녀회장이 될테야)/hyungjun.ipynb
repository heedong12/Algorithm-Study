    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    k = int(input())\n",
    "    n = int(input())\n",
    "    apart = [[i for i in range(1, n+1)] for _ in range(k+1)]\n",
    "    for i in range(1, k+1):\n",
    "        for j in range(1, n):\n",
    "            apart[i][j] = apart[i][j-1] + apart[i-1][j]\n",
    "    print(apart[k][n-1])"
   ]
