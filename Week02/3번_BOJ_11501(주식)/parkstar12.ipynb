    "#백준 11501번\n",
    "T=int(input())\n",
    "for test_case in range(T):\n",
    "  N=int(input())\n",
    "  result=0\n",
    "  max=0\n",
    "  price=[0]+list(map(int,input().split()))\n",
    "  for i in range (N,1,-1):\n",
    "    if max<price[i]:\n",
    "      max=price[i]\n",
    "    elif max>price[i]:\n",
    "      result+=max-price[i]\n",
    "  print(result)"
