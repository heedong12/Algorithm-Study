   "source": [
    "L,P,N=map(int,input().split())\n",
    "i=1\n",
    "while L!=0 and P!=0 and N!=0:\n",
    "  daycount=N//P\n",
    "  result=daycount*L\n",
    "  result+=min(N%P,L)\n",
    "  print(f\"Case {i}: {result}\")\n",
    "  i+=1\n",
    "  L,P,N=map(int,input().split())"
