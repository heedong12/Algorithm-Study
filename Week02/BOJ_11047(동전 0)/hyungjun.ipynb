   "source": [
    "N , K = map(int,input().split())\n",
    "coins = []\n",
    "for _ in range(N):\n",
    "    coins.append(int(input()))\n",
    "\n",
    "result = 0\n",
    "while(coins):\n",
    "    current = coins.pop()\n",
    "    if K // current != 0:\n",
    "        result += K // current\n",
    "        K = K % current\n",
    "    if K == 0:\n",
    "        break\n",
    "print(result)\n",
    "print(result)"
   ]
  }
