    "N = input().strip()\n",
    "flag=0\n",
    "sum_num=0\n",
    "\n",
    "for i in range(len(N)):\n",
    "  if N[i] == '0':\n",
    "    flag=1\n",
    "  sum_num += int(N[i])\n",
    "  \n",
    "if flag == 1 and sum_num%3 == 0:\n",
    "  result=sorted(N, reverse=True)\n",
    "  print(''.join(result))\n",
    "else:\n",
    "  print(-1)\n",
    "\n"
   ]
  }
