    "N , P , Q = map(int,input().split())\n",
    "\n",
    "dic ={ 0: 1, 1: 2}\n",
    "\n",
    "def A(i):\n",
    "    if dic.get(i):\n",
    "        return dic[i]\n",
    "    else:\n",
    "        dic[i] = A(i//P) + A(i//Q)\n",
    "        return dic[i]\n",
    "    \n",
    "print(A(N))"
   ]
  }
