   "source": [
    "N,P,Q=map(int,input().split())\n",
    "# 0 : 1\n",
    "# 1 : 1+1 =2\n",
    "# 2 : 2+1 =3\n",
    "# 3 : 2+2 = 4\n",
    "# 4 : 3+2 = 5\n",
    "# 5 : 3+2 = 5\n",
    "# 6 : 4+3 = 7\n",
    "# 7 : 4+3 = 7\n",
    "\n",
    "number = {0:1}\n",
    "\n",
    "def get_number(i):\n",
    "  if i in number:\n",
    "    return number[i]\n",
    "  return get_number(i//P) + get_number(i//Q)\n",
    "\n",
    "print(get_number(N))\n",
    "\n",
    "\n"
   ]
