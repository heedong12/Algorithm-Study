    "N=int(input())\n",
    "password=set()\n",
    "for _ in range(N):\n",
    "    word=input()\n",
    "    password.add(word)\n",
    "for i in password:\n",
    "    reverse_word=i[::-1]\n",
    "    if reverse_word in password:\n",
    "        length=len(reverse_word)\n",
    "        print(length)\n",
    "        print(reverse_word[length//2])\n",
    "        break\n",
    "     \n"
