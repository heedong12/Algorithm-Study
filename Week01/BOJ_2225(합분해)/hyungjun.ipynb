   "source": [
    "n = int(input())\n",
    "dp = [0 for _ in range(n+1)]\n",
    "for i in range(n+1):\n",
    "    if i > 1:\n",
    "        dp[i] = dp[i-1] + dp[i-2]\n",
    "    else:\n",
    "        dp[i] = i\n",
    "print(dp[n])"
   ]
