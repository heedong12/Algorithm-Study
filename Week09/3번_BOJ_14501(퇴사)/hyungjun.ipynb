    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def solve(node,n,price):\\n\",\n",
    "    \"    if node >= n :\\n\",\n",
    "    \"        result[0] = max(price,result[0])\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    for i in range(node,n):\\n\",\n",
    "    \"        if not visited[i]:\\n\",\n",
    "    \"            visited[i] = True\\n\",\n",
    "    \"            if i + arrT[i] <= n:\\n\",\n",
    "    \"                solve(i+arrT[i],n,price+arrP[i])\\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                solve(n+arrT[i],n,price)\\n\",\n",
    "    \"            visited[i] = False\\n\",\n",
    "    \"\\n\",\n",
    "    \"n = int(input())\\n\",\n",
    "    \"arrT = []\\n\",\n",
    "    \"arrP = []\\n\",\n",
    "    \"for _ in range(n):\\n\",\n",
    "    \"    t, p = map(int, input().split())\\n\",\n",
    "    \"    arrT.append(t)\\n\",\n",
    "    \"    arrP.append(p)\\n\",\n",
    "    \"result = [0]\\n\",\n",
    "    \"visited = [False] * n\\n\",\n",
    "    \"solve(0,n,0)\\n\",\n",
    "    \"print(result[0])\\n\"\n",
